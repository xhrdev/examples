{
  "$schema": "https://json.schemastore.org/package",
  "name": "@xhrdev/examples",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xhrdev/examples.git"
  },
  "type": "module",
  "scripts": {
    "depcheck": "depcheck",
    "eslint": "eslint .",
    "exec": "tsx",
    "repl": "tsx test/repl.ts",
    "build": "tspc -p tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "tsx": "NODE_TLS_REJECT_UNAUTHORIZED=0 tsx",
    "playwright": "tsx src/playwright.ts",
    "puppeteer": "tsx src/puppeteer.ts",
    "p": "npm run playwright",
    "prepare": "husky"
  },
  "private": true,
  "author": "XHR Software Inc",
  "dependencies": {
    "axios": "1.13.2",
    "axios-cookiejar-support": "6.0.5",
    "dotenv": "17.2.3",
    "fetch-cookie": "3.2.0",
    "http-cookie-agent": "7.0.3",
    "https-proxy-agent": "7.0.6",
    "node-fetch": "3.3.2",
    "tough-cookie": "6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-angular": "19.5.0",
    "@skilbjo/config-rc": "1.0.35",
    "@tsconfig/node22": "22.0.5",
    "@types/node": "25.0.10",
    "@types/tough-cookie": "4.0.5",
    "@typescript-eslint/eslint-plugin": "8.53.1",
    "@typescript-eslint/parser": "8.53.1",
    "cheerio": "1.1.2",
    "depcheck": "1.4.7",
    "eslint": "9.39.2",
    "extensionless": "2.0.6",
    "husky": "9.1.7",
    "nodemon": "3.1.11",
    "playwright": "1.57.0",
    "puppeteer": "24.35.0",
    "ts-patch": "3.3.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-transform-paths": "3.5.6",
    "undici": "7.19.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
